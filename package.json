    "babel-preset-minify": "^0.5.2",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "http-cache-semantics": ">=4.1.1",
    "jest": "^29.7.0",
    "json5": ">=2.2.3",
