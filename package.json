    "mysql": "^2.18.1",
    "node-cache": "^5.1.2",
    "redis": "^4.6.14",
    "robust-validator": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.6",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/runtime": "^7.24.6",
    "@faker-js/faker": "^8.4.1",
    "@tsconfig/recommended": "^1.0.3",
